  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-mongo-sanitize": "^2.1.0",
    "mapbox-gl": "^2.3.1",
    "mongoose": "^5.12.14",
    "node-geocoder": "^3.27.0"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "concurrently": "^6.2.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7"
  }
}
