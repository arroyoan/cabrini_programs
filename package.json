{
  "name": "cabrini_map_list",
  "type": "module",
  "version": "1.0.0",
  "description": "Application that displays programs affiliated with Cabrini.",
  "main": "server.js",
  "scripts": {
    "start":"node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
